{
  "name": "chatwell",
  "version": "0.2.2",
  "description": "A chat app for people with experience of mental health issues.",
  "repository": "https://github.com/pohutukawa-2019/chatwell.git",
  "main": "server/index.js",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "dev": "nodemon server & webpack --watch",
    "server": "node server",
    "start": "webpack && nodemon server",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db:reset": "run-s db:delete db:migrate db:seed",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:delete": "rm -f server/db/dev.sqlite3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.4.3",
    "acorn": "^7.1.0",
    "authenticare": "^0.4.5",
    "express": "^4.17.1",
    "knex": "^2.4.0",
    "material-ui": "^0.20.2",
    "npm-run-all": "^4.1.5",
    "react-player": "^1.13.0",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.0",
    "react-router-dom": "^5.1.0",
    "react-window": "^1.8.5",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "semantic-ui-react": "^0.88.1",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sqlite3": "^4.1.0",
    "styled-components": "^4.4.0",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jest": "^24.9.0",
    "nodemon": "^1.19.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  }
}
